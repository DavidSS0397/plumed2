d1: READ FILE=swiss.dat VALUES=c1
d2: READ FILE=swiss.dat VALUES=c2
d3: READ FILE=swiss.dat VALUES=c3

ff: COLLECT_FRAMES ARG1=d1 ARG2=d2 ARG3=d3 CLEAR=50
mds: CLASSICAL_MDS ARG=ff NLOW_DIM=2

weights: CUSTOM ARG1=ff.logweights FUNC=exp(x) PERIODIC=NO
smap: SKETCHMAP_CONJGRAD ...
  ARG1=mds-1 ARG2=mds-2 DISSIMILARITIES=mds_mat WEIGHTS=weights
  HIGH_DIM_FUNCTION={SMAP R_0=4 A=3 B=2}
  LOW_DIM_FUNCTION={SMAP R_0=4 A=1 B=2}
... 

PRINT ARG=smap.*,weights FMT=%8.4f FILE=smap STRIDE=50
